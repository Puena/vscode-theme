{
	"name": "my-theme",
	"colors": {
		"editor.background": "#f3f3f3",
		"editor.foreground": "#000000",
        "list.activeSelectionIconForeground": "#f3f3f3",
		"statusBar.background": "#8204da",
		"statusBarItem.remoteBackground": "#8204da",
		"activityBar.background": "#f3f3f3",
		"activityBar.foreground": "#000000",
		"titleBar.activeBackground": "#f3f3f3",
		"list.activeSelectionBackground": "#8104da8f",
		"list.activeSelectionForeground": "#000000",
		"list.focusForeground": "#000000",
		"list.focusHighlightForeground": "#000000",
		"list.focusAndSelectionOutline": "#8104da8f",
		"list.highlightForeground": "#000000",
		"list.focusOutline": "#8104da8f",
		"sideBarSectionHeader.background":"#f3f3f3",
		"badge.background": "#8104da8f",
		"badge.foreground": "#000000",
		"activityBarBadge.background":"#8204da",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"parameter": {
			"fontStyle": "underline",
		},
		"lifetime": {
			"foreground": "#cf0000",
			"fontStyle": "bold",
		},
		"selfKeyword": {
			"fontStyle": "bold"
		},
		"selfTypeKeyword": {
			"fontStyle": "bold"
		},
		"keyword": {
			"foreground": "#8204da"
		},
		// "function.static": {
		// 	"foreground": "#920000",
		// 	"fontStyle": "",
		// },
		// "function.library": {
		// 	"foreground": "#920000",
		// 	"fontStyle": ""
		// },
		// "method.defaultLibrary": {
		// 	"foreground": "#8204da",
		// 	"fontStyle": "",
		// }
	},
	"tokenColors": [
		// 
		{
			"name": "Punctuation brackets",
			"scope": [
				"punctuation.brackets",
			],
			"settings": {
				"foreground": "#8204da",
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted"
			],
			"settings": {
				"foreground": "#157e00",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#aaaaaa",
				"fontStyle": ""
			}
		},
		{
			"name": "Bytes",
			"scope": [
				"string.quoted.byte.raw"
			],
			"settings": {
				"foreground": "#920000",
				"fontStyle": ""
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#5b84fc",
				"fontStyle": ""
			}
		},
		{
			"name": "Boolean",
			"scope": [
				"constant.language.bool",
			],
			"settings": {
				"foreground": "#cf0000",
				"fontStyle": ""
			}
		},
		// {
		// 	"name": "Namespace",
		// 	"scope": [
		// 		"entity.name.namespace",
		// 	],
		// 	"settings": {
		// 		"foreground": "#920000",
		// 		"fontStyle": ""
		// 	}
		// },
		{
			"name": "Attributes",
			"scope": [
				"source.rust meta.attribute",
				"source.rust punctuation.brackets.attribute",
			],
			"settings": {
				"foreground": "#ff7b00",
				// "fontStyle": "bold"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"source.rust keyword",
				"source.ts keyword",
			],
			"settings": {
				"foreground": "#8204da",
				"fontStyle": ""
			}
		},
		{
			"name": "Super",
			"scope": [
				"source.ts variable.language.super",
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Fucntion definition",
			"scope": [
				"meta.function.definition entity.name.function",
				"source.ts meta.definition.method",
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Fucntion definition call",
			"scope": [
				"meta.function.definition meta.function.call entity.name.function",
			],
			"settings": {
				"fontStyle": ""
			}
		},
		// {
		// 	"name": "Entity type",
		// 	"scope": [
		// 		"entity.name.type",
		// 	],
		// 	"settings": {
		// 		// "foreground": "#920000",
		// 		"fontStyle": "",
		// 	}
		// },
		// Rust
		{
			"name": "Rust option",
			"scope": [
				"source.rust entity.name.type.option",
			],
			"settings": {
				"foreground": "#8204da",
				"fontStyle": ""
			}
		},
		{
			"name": "Rust result",
			"scope": [
				"source.rust entity.name.type.result",
			],
			"settings": {
				"foreground": "#8204da",
				"fontStyle": ""
			}
		},
		{
			"name": "Rust macro",
			"scope": [
				"source.rust entity.name.function.macro",
			],
			"settings": {
				"foreground": "#FF7b00",
				// "fontStyle": "bold"
			}
		},
		// HTML
		{
			"name": "HTML Tags",
			"scope": [
				"text.html entity.name.tag",
			],
			"settings": {
				"foreground": "#FF7b00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML attribute name",
			"scope": [
				"text.html entity.other.attribute-name",
			],
			"settings": {
				"foreground": "#920000",
				"fontStyle": "",
			}
		},
		// JSON
		{
			"name": "JSON Constant",
			"scope": [
				"source.json constant.language"
			],
			"settings": {
				"foreground": "#920000",
				"fontStyle": "",
			}
		},
		// TOML
		{
			"name": "TOML group",
			"scope": [
				"source.toml support.type.property-name.table"
			],
			"settings": {
				"foreground": "#920000",
				"fontStyle": "bold"
			}
		},
		// Typescript
		{
			"name": "Typescript type, modifiers",
			"scope": [
				"source.ts storage.type",
				"source.ts storage.modifier"
			],
			"settings": {
				"foreground": "#8204da",
				"fontStyle": ""
			}
		},
		{
			"name": "Typescript this keyword",
			"scope": [
				"source.ts variable.language.this",
			],
			"settings": {
				"fontStyle": "bold"
			}
		}
	],
